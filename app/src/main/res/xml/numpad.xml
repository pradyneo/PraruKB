<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="55dp"
    android:keyWidth="10%p">

    <Row>
        <Key android:codes="64" android:keyLabel="\@" android:keyEdgeFlags="left"/>
        <!--<Key android:codes="35" android:keyLabel="\#" />-->
        <Key android:codes="63" android:keyLabel="\?"/>
        <Key android:codes="36" android:keyLabel="$"/>
        <Key android:codes="49" android:keyLabel="1" android:horizontalGap="5%p"/>
        <Key android:codes="50" android:keyLabel="2" />
        <Key android:codes="51" android:keyLabel="3" />
        <Key android:codes="43" android:keyLabel="+" android:horizontalGap="5%p"/>
        <Key android:codes="37" android:keyLabel="%" />
        <Key android:codes="38" android:keyLabel="&amp;"/>
        <!--<Key android:codes="42" android:keyLabel="*" android:keyEdgeFlags="right"/>-->
    </Row>

    <Row>
        <Key android:codes="34" android:keyLabel="&quot;" android:keyEdgeFlags="left"/>
        <Key android:codes="95" android:keyLabel="_" />
        <Key android:codes="92" android:keyLabel="\\" />
        <Key android:codes="52" android:keyLabel="4" android:horizontalGap="5%p"/>
        <Key android:codes="53" android:keyLabel="5" />
        <Key android:codes="54" android:keyLabel="6" />
        <Key android:codes="45" android:keyLabel="-" android:horizontalGap="5%p"/>
        <Key android:codes="40" android:keyLabel="("/>
        <Key android:codes="46" android:keyLabel="." android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="39" android:keyLabel="\'" android:keyEdgeFlags="left"/>
        <Key android:codes="58" android:keyLabel=":"/>
        <Key android:codes="37" android:keyLabel="%"/>
        <Key android:codes="55" android:keyLabel="7" android:horizontalGap="5%p"/>
        <Key android:codes="56" android:keyLabel="8" />
        <Key android:codes="57" android:keyLabel="9" />
        <Key android:codes="59" android:keyLabel=";" android:horizontalGap="5%p"/>
        <Key android:codes="41" android:keyLabel=")" />
        <Key android:codes="-5" android:keyIcon="@drawable/backspace" android:isRepeatable="true" android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="-2" android:keyLabel="abc" android:keyWidth="12%p" android:keyEdgeFlags="left"/>
        <Key android:codes="47" android:keyLabel="/" android:keyWidth="9%p" />
        <Key android:codes="44" android:keyLabel="," android:keyWidth="9%p" />
        <Key android:codes="42" android:keyLabel="*" android:horizontalGap="5%p" />
        <!--<Key android:codes="63" android:keyLabel="\?"/>-->
        <Key android:codes="48" android:keyLabel="0"  />
        <Key android:codes="35" android:keyLabel="\#" />

        <Key android:codes="32" android:keyLabel=" " android:keyWidth="20%p"  android:horizontalGap="5%p" android:isRepeatable="true"/>
        <Key android:codes="-4" android:keyIcon="@drawable/enter" android:keyWidth="10%p" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>